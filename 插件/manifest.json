{
  "manifest_version": 3,
  "name": "AI Chat Assistant",
  "version": "1.0",
  "description": "AI聊天助手 - 支持多个AI模型对话",
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "AI Chat Assistant"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
} 